# Uncomment the next line to define a global platform for your project
platform :osx, '10.10'

target 'Hoverboard' do
    # Comment the next line if you're not using Swift and don't want to use
    # dynamic frameworks
    use_frameworks!

    # Pods for Hoverboard
    pod 'ElsloooKit', :podspec => 'https://elsl.ooo/pods/elslooo-kit/podspec'
    pod 'HockeySDK-Mac'
    pod 'LetsMove'
    pod 'Sparkle'
end
